<!--index.wxml-->
<map 
  id='myMap'
  style='width: 100%; height: 100%'
  longitude='{{longitude}}'
  latitude='{{latitude}}'
  controls='{{controls}}'
  show-location='true'
  scale='{{scale}}'
  bindcontroltap='controltap'
  markers="{{markers}}"
  polyline="{{polyline}}"
  bindmarkertap="markertap"
  bindregionchange='regionchange'
>
</map>
